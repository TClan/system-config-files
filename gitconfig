[push]
  default = tracking
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
[format]
  pretty = %Cblue%h%Creset %Cgreen[%ar]%Creset (%an) %s
[alias]
  ci = commit -m
  st = status
  di = diff
  dic = diff --cached
  rh = reset --hard
  co = checkout
  br = branch
  pl = pull
  ps = push
  plre = pull --rebase
  out = log origin..HEAD
  ci = commit
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[user]
  name = Akshay Rawat
  email = projects@akshay.cc
[branch "master"]
    mergeoptions = --no-ff
[branch "develop"]
    mergeoptions = --no-ff